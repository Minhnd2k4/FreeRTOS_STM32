#ifndef DRIVER_LAYER_GPIO_MY_GPIO_H_
#define DRIVER_LAYER_GPIO_MY_GPIO_H_

#include "Gpio_Config.h"

void GreenLed_On(GPIO_TypeDef *GPIOx, uint16_t GPIO_Pin);
void GreenLed_Off(GPIO_TypeDef *GPIOx, uint16_t GPIO_Pin);
void RedLed_On(GPIO_TypeDef *GPIOx, uint16_t GPIO_Pin);
void RedLed_Off(GPIO_TypeDef *GPIOx, uint16_t GPIO_Pin);
void BlueLed_On(GPIO_TypeDef *GPIOx, uint16_t GPIO_Pin);
void BlueLed_Off(GPIO_TypeDef *GPIOx, uint16_t GPIO_Pin)


#endif /* DRIVER_LAYER_GPIO_MY_GPIO_H_ */
